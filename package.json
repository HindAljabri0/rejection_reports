{
  "name": "waseele-gui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start2G": "node --max-old-space-size=2049 ./node_modules/@angular/cli/bin/ng serve --aot",
    "start4G": "node --max-old-space-size=4040 ./node_modules/@angular/cli/bin/ng serve --aot",
    "start8G": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --aot",
    "startAr": "ng serve --configuration=dev-ar",
    "startQAAr": "ng serve --configuration=qa-ar",
    "startQAEn": "ng serve --configuration=qa-en",
    "int:extractAr": "ng xi18n --i18n-locale ar --out-file src/locale/messages.ar.xlf",
    "buildEn": "ng build --configuration=qa-en && npm run post-build -- en",
    "buildAr": "ng build --configuration=qa-ar && npm run post-build -- ar",
    "buildStgEn": "ng build --configuration=staging-en && npm run post-build -- en",
    "buildStgAr": "ng build --configuration=staging-ar && npm run post-build -- ar",
    "buildProdEn": "ng build --prod",
    "buildProdAr": "ng build --configuration=production-ar",
    "build": "npm-run-all --parallel buildOciQAEn buildOciQAAr",
    "buildStg": "npm-run-all --parallel buildStgEn buildStgAr",
    "buildProd": "npm-run-all --parallel buildProdEn buildProdAr",
    "buildDREn": "ng build --configuration=dr-en && npm run post-build -- en",
    "buildDRAr": "ng build --configuration=dr-ar && npm run post-build -- ar",
    "buildOciQAEn": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build --configuration=oci_qa-en --aot && npm run post-build -- en",
    "buildOciQAAr": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build --configuration=oci_qa-ar --aot && npm run post-build -- ar",
    "buildOciStgEn": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build --configuration=oci_staging-en && npm run post-build -- en",
    "buildOciStgAr": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build --configuration=oci_staging-ar && npm run post-build -- ar",
    "buildOciProdEn": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build --configuration=oci_prod-en && npm run post-build -- en",
    "buildOciProdAr": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build --configuration=oci_prod-ar && npm run post-build -- ar",
    "buildDR": "npm-run-all --parallel buildDREn buildDRAr",
    "buildOciQA": "npm-run-all --parallel buildOciQAEn",
    "buildOciStg": "npm-run-all buildOciStgEn",
    "buildOciProd": "npm-run-all buildOciProdEn",
    "post-build": "node ./build/post-build.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.12",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.12",
    "@angular/compiler": "^8.2.12",
    "@angular/core": "^8.2.12",
    "@angular/forms": "^8.2.12",
    "@angular/material": "^8.2.3",
    "@angular/material-moment-adapter": "^8.2.3",
    "@angular/platform-browser": "^8.2.12",
    "@angular/platform-browser-dynamic": "^8.2.12",
    "@angular/router": "^8.2.12",
    "@auth0/angular-jwt": "^3.0.1",
    "@elastic/apm-rum-angular": "^0.4.3",
    "@material-git/grid-list": "^2.0.0-git.20160919",
    "@material/card": "^3.2.0",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "@ngrx/store-devtools": "^9.2.0",
    "all": "0.0.0",
    "animate.css": "^4.1.1",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.9.4",
    "chartjs-plugin-datalabels": "^0.7.0",
    "chartjs-plugin-piechart-outlabels": "^0.1.4",
    "core-js": "^3.3.4",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.8",
    "jszip": "3.7.1",
    "moment": "^2.24.0",
    "ng-event-source": "^1.0.14",
    "ng2-charts": "^2.4.2",
    "ng2-file-upload": "^1.4.0",
    "ngx-bootstrap": "^5.5.0",
    "ngx-chips": "~2.1.0",
    "ngx-clipboard": "^12.3.1",
    "ngx-guided-tour": "^1.1.11",
    "ngx-infinite-scroll": "^8.0.1",
    "ngx-json-viewer": "^3.0.2",
    "ngx-mat-select-search": "^3.2.0",
    "ngx-monaco-editor": "^8.1.1",
    "ngx-owl-carousel-o": "^2.1.1",
    "ngx-scrollbar": "^4.2.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "typescript": "3.5.3",
    "xlsx": "^0.16.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.24",
    "@angular/cli": "^8.3.14",
    "@angular/compiler-cli": "^8.2.12",
    "@angular/language-service": "^8.2.12",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "protractor": "^7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "^3.5.3"
  }
}
